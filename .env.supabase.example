# Supabase Configuration for TTB Pipeline
# Copy this file to .env.local and add your actual Supabase credentials

# Supabase Project Configuration
SUPABASE_URL=https://<instance>.supabase.co
SUPABASE_KEY=supabase-anon-public-key

# PostgreSQL Connection (for bulk operations)
# You can find this in Supabase Dashboard > Settings > Database
POSTGRES_URL=postgres-url-for-supabase-instance

# Existing TTB Configuration (keep these as they are)
TTB_S3_BUCKET=bucket-name
AWS_REGION=region-name
AWS_ACCESS_KEY_ID=your-aws-access-key
AWS_SECRET_ACCESS_KEY=your-aws-secret-key

# Dagster Configuration
DAGSTER_ENVIRONMENT=development
